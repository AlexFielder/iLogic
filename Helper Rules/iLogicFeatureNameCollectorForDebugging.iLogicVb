Sub main()
	Dim ofeature As Face = ThisApplication.CommandManager.Pick(SelectionFilterEnum.kPartFaceFilter, "Select first feature")
	
	Dim referenceKeyManager As ReferenceKeyManager = ThisDoc.Document.ReferenceKeyManager
	Dim ReferenceKey = New Byte() {}
	If Not ofeature Is Nothing Then
		ofeature.CreatedByFeature.GetReferenceKey(ReferenceKey)
		Dim ReferenceKey2 As Byte() = ReferenceKey
		MessageBox.Show(referenceKeyManager.KeyToString(ReferenceKey2) & vbTab)
	End If
End Sub
